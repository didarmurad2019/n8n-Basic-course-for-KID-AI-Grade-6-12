{
  "name": "Simple Math Chatbot",
  "nodes": [
    {
      "parameters": {},
      "id": "manual-trigger",
      "name": "Ask Question",
      "type": "n8n-nodes-base.manualTrigger",
      "typeVersion": 1,
      "position": [240, 300],
      "webhookId": "ask-question"
    },
    {
      "parameters": {
        "options": {}
      },
      "id": "switch-node",
      "name": "Check Question",
      "type": "n8n-nodes-base.switch",
      "typeVersion": 1,
      "position": [440, 300],
      "rules": [
        {
          "conditions": {
            "string": [
              {
                "value1": "={{ $json.body }}",
                "operation": "contains",
                "value2": "math"
              }
            ]
          }
        },
        {
          "conditions": {
            "string": [
              {
                "value1": "={{ $json.body }}",
                "operation": "contains",
                "value2": "science"
              }
            ]
          }
        }
      ],
      "defaultOutput": "default"
    },
    {
      "parameters": {
        "message": "Here's a math fact: 2 + 2 = 4! üßÆ"
      },
      "id": "math-response",
      "name": "Math Answer",
      "type": "n8n-nodes-base.set",
      "typeVersion": 1,
      "position": [640, 200]
    },
    {
      "parameters": {
        "message": "Did you know that water freezes at 0¬∞C? ‚ùÑÔ∏è"
      },
      "id": "science-response",
      "name": "Science Answer",
      "type": "n8n-nodes-base.set",
      "typeVersion": 1,
      "position": [640, 300]
    },
    {
      "parameters": {
        "message": "I'm not sure how to answer that. Try asking about math or science! ü§î"
      },
      "id": "default-response",
      "name": "Default Answer",
      "type": "n8n-nodes-base.set",
      "typeVersion": 1,
      "position": [640, 400]
    }
  ],
  "connections": {
    "manual-trigger": {
      "main": [[["switch-node"]]]
    },
    "switch-node": {
      "main": [
        [["math-response"]],
        [["science-response"]],
        [["default-response"]]
      ]
    }
  }
}